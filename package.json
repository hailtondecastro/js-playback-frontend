{
  "name": "ng-js-hb-supersync",
  "version": "0.0.1",
  "description": "Synchronization framework between angular and hibernate",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clear-all && npm run build:prepare-dist-folder && tsc --p tsconfig-dist.json --extendedDiagnostics && npm run build:embed-source-contents",
    "build-fast": "tsc --p tsconfig-dist.json --extendedDiagnostics && npm run build:embed-source-contents",
    "clear-all": "node ./build-utils/clear-all -baseDistFolder ./dist -verbose true",
    "build:embed-source-contents": "node ./build-utils/embed-source-contents -baseDistFolder ./dist -verbose true",
    "build:prepare-dist-folder": "node ./build-utils/prepare-dist-folder -baseDistFolder ./dist -verbose true"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.117",
    "@types/rx": "^4.1.1",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "mkdirp": "^0.5.1",
    "replace-in-file": "^3.4.2",
    "source-map": "^0.7.3",
    "typescript": "~2.7.2"
  },
  "dependencies": {
    "@angular/common": "^6.0.6",
    "@angular/core": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@types/node": "^10.3.5",
    "lodash": "^4.17.5",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.3.3"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "sideEffects": false
}
