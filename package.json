{
  "name": "ng-js-hb-supersync",
  "version": "0.0.1",
  "description": "Synchronization framework between angular and hibernate",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run clear-all && npm run build:prepare-dist-folder && npm run build:ngc && npm run build:embed-source-contents",
    "build:ngc": "tsc -p tsconfig-dist.json && echo 'Build Complete'",
    "build-fast": "tsc -p tsconfig-dist.json --extendedDiagnostics && npm run build:embed-source-contents",
    "build-test": "tsc -p tsconfig.json --extendedDiagnostics && node ./build-utils/copy-recursive -baseFolder . -relativeSourceFolder test -relativeTargetFolder dist_dev -extension txt -verbose true",
    "clear-all": "node ./build-utils/clear-all -baseDistFolder ./dist -verbose true && node ./build-utils/clear-all -baseDistFolder ./dist_dev -verbose true",
    "build:embed-source-contents": "node ./build-utils/embed-source-contents -baseDistFolder ./dist -verbose true",
    "build:prepare-dist-folder": "node ./build-utils/prepare-dist-folder -baseDistFolder ./dist -verbose true",
    "test": "npm run clear-all && npm run build-test && mocha dist_dev/**/**.spec.js",
    "test2": "npm run clear-all && npm run build-test && mocha --grep /master-lazy-prp-over-sized-test/ dist_dev/**/**.spec.js",
    "browserify": "npm run clear-all && npm run build-test && browserify dist_dev/index.js --outfile browser.js"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.0.3",
    "@types/chai": "^4.1.7",
    "@types/jasmine": "^2.5.43",
    "@types/lodash": "^4.14.117",
    "@types/rx": "^4.1.1",
    "@types/stream-buffers": "^3.0.3",
    "@types/uuid": "^3.4.4",
    "atob": "^2.1.2",
    "browserify": "^16.2.3",
    "btoa": "^1.2.1",
    "chai": "^4.2.0",
    "filereader": "^0.10.3",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "replace-in-file": "^3.4.2",
    "source-map": "^0.7.3",
    "stream-to-observable": "^0.2.0",
    "ts-node": "^8.1.0",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "@angular/common": "^6.0.6",
    "@angular/core": "^6.1.10",
    "@types/node": "^10.3.5",
    "base64-stream": "^1.0.0",
    "blob-to-stream": "^1.0.3",
    "get-stream": "^5.1.0",
    "lodash": "^4.17.5",
    "memory-streams": "^0.1.3",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.3.3",
    "stream-buffers": "^3.0.2",
    "stream-to-blob": "^1.0.1",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "sideEffects": false
}
